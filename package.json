{
  "name": "dynamic-font-size",
  "version": "1.0.0",
  "description": "Dynamically adjust font size of note content using a property slider",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
