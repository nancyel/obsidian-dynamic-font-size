  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.7",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.2",
    "obsidian": "latest",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
